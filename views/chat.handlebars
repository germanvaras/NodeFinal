<h1>Chat</h1>
<section id="containerChat" class="containerChat">
    <div id="messages" >
    </div>
    <form id="chatForm" class="chatForm">
        <input type="text" name="user" id="user" class="formInput" placeholder="{{this.user.username}}" value="{{this.user.username}}" readonly >
        <input type="text" name="message" id="userMessage"  class="formInput" placeholder="Mensaje">
        <button type="submit" class="buttonForm">Enviar</button>
    </form>
</section>
<script src="/socket.io/socket.io.js"></script>
<script src="/js/chat.js"></script>